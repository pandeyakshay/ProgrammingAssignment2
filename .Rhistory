columnmean<-function(y)
{
nc<-ncol(y)
means<-numeric(nc)
for(i in 1:nc)
{
means[i]<-mean(y[,i])
}
means
}
columnmean(airquality)
columnmean<-function(y, removeNA=TRUE)
{
nc<-ncol(y)
means<-numeric(nc)
for(i in 1:nc)
{
means[i]<-mean(y[,i], na.rm = removeNA)
}
means
}
columnmean
columnmean(airquality)
columnmean(airquality, FALSE)
